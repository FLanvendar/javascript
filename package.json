{
  "name": "javascript",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "bulid": "node index.js"
  },
  "dependencies": {
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0"
  },
  "devDependencies": {},
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "rules": {
      "quotes": "double",
      "no-console": 0,
      "eqeqeq": 0,
      "semi": [
        0
      ],
      "no-irregular-whitespace": 0,
      "no-multi-spaces": 0,
      "no-trailing-spaces": 0,
      "no-unneeded-ternary": 0,
      "no-underscore-dangle": 0,
      "no-undef-init": 0,
      "no-undefined": 0,
      "no-unexpected-multiline": 0,
      "quote-props": [
        0
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-px-to-viewport": {
        "viewportWidth": 750,
        "viewportHeight": 1334,
        "unitPrecision": 3,
        "viewportUnit": "vw",
        "selectorBlackList": [
          ".ignore",
          ".hairlines"
        ],
        "minPixelValue": 1,
        "mediaQuery": false
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "description": "广告投放页面"
}
